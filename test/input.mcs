		say Number $i;
	};

	foreach(var $i in range(5)){
		say again $i;
	};

	execute @a ~ ~ ~ {
		execute @e 1 1 1 {
			say evenmore;
		};
		say hey;
	};

	say "an evaled block `return aMacro(5);`";

};

